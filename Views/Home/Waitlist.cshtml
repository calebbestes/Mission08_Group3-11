@{
}

@model List<Application>
<table class="table table-bordered">

    <thead class="fw-bold">
     <tr>
          <td>Task</td>
          <td>DueDate</td>
          <td>Quadrant</td>
          <td>Category</td>
          <td>Completed</td>
    </tr>

   </thead>

@foreach (var x in Model)
{
<tbody>
    <tr>
        <td>@x.Task</td>
        <td>@x.DueDate</td>
        <td>@x.Quadrant</td>
        <td>@x.Category</td>
        <td>@x.Completed</td>
        
    </tr>
  

</tbody>
}

</table>